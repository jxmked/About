{
  "name": "jxmked",
  "version": "1.0.4",
  "description": "Personal Portfolio",
  "main": "./dist/main.js",
  "scripts": {
    "minify": "uglifyjs dist/index.js --mangle toplevel=true -O quote_keys=true -o dist/index.js",
    "build": "tsc -p tsconfig-build.json && sass -s compressed --no-source-map \"./src/styles/main.scss:dist/main.css\" && npm run minify",
    "dev": "concurrently --raw \"tsc --watch -p tsconfig.json\" \"sass --watch --embed-sources \"./src/styles/main.scss:dist/main.css\"\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxmked/jxmked.git"
  },
  "keywords": [
    "portfolio",
    "personal-porfolio"
  ],
  "author": "jxmked <jamc015lol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jxmked/jxmked/issues"
  },
  "homepage": "https://jxmked.github.io/jxmked/",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@types/localforage": "0.0.33",
    "@vercel/ncc": "^0.34.0",
    "concurrently": "^7.4.0",
    "modernizr": "^3.12.0",
    "sass": "^1.54.9",
    "typescript": "^4.8.2",
    "uglify-js": "^3.17.4"
  }
}
